{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "actn-front-end": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/actn-front-end"
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.json",
              "src/manifest.json",
              "src/manifest.json",
              "src/manifest.json",
              "src/manifest.json",
              "src/manifest.json",
              "src/manifest.json",
              "src/manifest.json",
              "src/manifest.json",
              "src/sitemap.xml"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/@fortawesome/fontawesome-svg-core/styles.css",
              "src/style/_core/styles.scss",
              "node_modules/ngx-red-zoom/styles/_base.scss",
              "node_modules/ngx-red-zoom/styles/_style-magnifier.scss",
              "node_modules/ngx-red-zoom/styles/mixins/_style-magnifier.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/style/_core",
                "src/style/"
              ]
            },
            "scripts": [
            ],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "@babel/runtime-corejs3"
            ],
            "serviceWorker": "ngsw-config.json",
            "browser": "src/main.ts",
            "server": "src/main.server.ts",
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "development": {
              "outputPath": "dist/actn-front-end",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": true,
                "vendor": true
              },
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "400mb",
                  "maximumError": "1000mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ]
            },
            "production": {
              "outputPath": "dist/actn-front-end",

              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": true,
                "vendor": true
              },
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "400mb",
                  "maximumError": "1000mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ]
            },
            "alpha-test": {
              "outputPath": "dist/actn-front-end",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.Alpha-test.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": true,
                "vendor": true
              },
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "400mb",
                  "maximumError": "1000mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ]
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "400mb",
                  "maximumError": "1000mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "1000kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "actn-front-end:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "actn-front-end:build:production"
            },
            "development": {
              "buildTarget": "actn-front-end:build:development"
            },
            "alpha-test": {
              "buildTarget": "actn-front-end:build:alpha-test"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "actn-front-end:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/style/_core/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/style/",
                "src/style/_core"
              ]
            },
            "scripts": [
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/sitemap.xml",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest",
              "src/manifest.webmanifest"
            ]
          }
        }
      }
    },
    "actn-front-end-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "actn-front-end:serve"

          },
          "configurations": {
            "production": {
              "devServerTarget": "actn-front-end:serve:production"
            }
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss",
      "standalone": false
    }
  },
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": false
    }
  }
}
